    "tango-controls",
    "tango-rest",
    "module",
    "waltz-plugin"
  ],
  "author": "Ingvord",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ingvord/axsis-gui/issues"
  },
  "homepage": "https://github.com/ingvord/axsis-gui",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "archiver": "^3.1.1",
    "atob": "^2.1.2",
    "babel-loader": "^8.1.0",
    "btoa": "^1.2.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "express": "^4.17.1",
    "file-loader": "^0.11.2",
    "fs": "0.0.1-security",
