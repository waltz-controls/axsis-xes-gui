    "mocha-webpack": "2.0.0-beta.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^4.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@waltz-controls/middleware": "^2.0.0"
